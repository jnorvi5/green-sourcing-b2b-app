{
  "name": "green-sourcing-b2b-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "check:links": "lychee --config lychee.toml '*.md' '*.html' 'docs/**/*.md' 'aws/**/*.md' 'database-schemas/**/*.md' 'marketing/**/*.md' '.github/**/*.md'",
    "test": "jest",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "vercel-build": "next build",
    "start:all": "concurrently \"npm start\" \"cd frontend && npm run dev\" \"cd backend && npm start\"",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd .."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@sentry/nextjs": "^10.29.0",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.39.0",
    "autoprefixer": "^10.4.16",
    "fuse.js": "^7.0.0",
    "iconv-lite": "^0.7.0",
    "mongodb": "^6.21.0",
    "mongoose": "^8.20.2",
    "next": "^14.2.33",
    "openai": "^4.20.1",
    "pdfkit": "^0.14.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.68.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.10.3",
    "resend": "^6.6.0",
    "stripe": "^14.9.0",
    "tailwindcss": "^3.4.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.6",
    "@types/pdfkit": "^0.13.4",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  }
}
