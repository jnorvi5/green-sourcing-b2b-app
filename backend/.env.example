# Database
PORT=3001
# SUPABASE_URL and SUPABASE_ANON_KEY needed for auth/edge functions
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
DATABASE_URL=postgres://user:password@host:port/database

# Frontend
FRONTEND_URL=http://localhost:5173

# Azure Integrations
# Application Insights
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=...;IngestionEndpoint=...

# Azure Cache for Redis
REDIS_HOST=greenchainz.redis.cache.windows.net
REDIS_KEY=your-redis-access-key

# Azure Key Vault
KEY_VAULT_NAME=greenchianz-vault
# If running locally, you might need AZURE_TENANT_ID, AZURE_CLIENT_ID, AZURE_CLIENT_SECRET
# On Azure, Managed Identity handles this automatically.
